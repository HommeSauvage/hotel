{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "ESNext",
    "esModuleInterop": true,
    "strict": true ,
    "noImplicitAny": true ,
    "strictNullChecks": true ,
    "strictFunctionTypes": true ,
    "strictBindCallApply": true ,
    "strictPropertyInitialization": true ,
    "noImplicitThis": true ,
    "alwaysStrict": true ,
    "noUnusedLocals": true,
  },
  "exclude": ["node_modules"]
}